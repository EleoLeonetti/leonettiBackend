<section>
    <h1>Lista de Productos {{ name }}</h1>
    {{#each products}}
    <div>
        <h3>{{ this.title}}</h3>
        <p>{{this.description}}</p>
        <p>Categoría: {{this.category}}</p>
        <p>Código: {{this.code}}</p>
        <p>Precio: ${{this.price}}.-</p>
        <p>Stock: {{this.stock}}</p>
        <hr>
    </div>
    {{/each}}
</section>